package abhi;
import java.util.scanner;

import org.omg.PortableInterceptor.SYSTEM_EXCEPTION;
public class Abhi{

	public static void main(String[] args) {
    Sanner=new Scanner(System.in);
    System.out.println("1.decimal no:,\n2.hexadeciml:\n3.octal\n4.exit");
    int num;
    do {
    	
    	
    }


    switch(num) { 
    case 1:
       int a=20;
       System.out.printf("decimal no %d/n",a);
     break;
     
    case 2:
        int b=30;
        System.out.printf("octolno %o/n",b);
       break;
    
    case 3:
      int c=40;
      System.out.printf("hexa decimal %x/n",c);
    break;
      
    case 4:
       float d=60;
       System.out.println("%f /n",d);
       
    case 5:
    	System.out.println("format specifire end");
    	break;
    	
    case 6:
    	System.exit(0);
    	
   default:
	   System.out.println("enter correct choice");
    }while(num!=6;)

	}

}
